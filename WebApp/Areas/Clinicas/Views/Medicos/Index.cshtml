@model IEnumerable<WebApp.Areas.Clinicas.Models.medicos>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Clinicas/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.colegiado_no)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.apellidos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.celular)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tel_casa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tel_otro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.clinicas.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.colegiado_no)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.apellidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.celular)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tel_casa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tel_otro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.correo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.clinicas.nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idMedico }) |
            @Html.ActionLink("Details", "Details", new { id=item.idMedico }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idMedico })
        </td>
    </tr>
}

</table>
